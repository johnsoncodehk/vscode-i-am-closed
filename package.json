{
	"name": "vscode-i-am-closed",
	"version": "0.0.2",
	"repository": {
		"type": "git",
		"url": "https://github.com/johnsoncodehk/vscode-i-am-closed.git"
	},
	"sponsor": {
		"url": "https://github.com/sponsors/johnsoncodehk"
	},
	"icon": "images/icon.png",
	"displayName": "I'm Closed",
	"description": "Show you who closed",
	"author": "johnsoncodehk",
	"publisher": "johnsoncodehk",
	"engines": {
		"vscode": "^1.76.0"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"configuration": {
			"title": "I'm Closed",
			"type": "object",
			"properties": {
				"iAmClosed.showOnlySelectedLine": {
					"type": "boolean",
					"default": true,
					"description": "Show only selected line"
				}
			}
		}
	},
	"main": "out/extension",
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"pack": "yarn build && vsce package",
		"release": "yarn build && vsce publish"
	},
	"devDependencies": {
		"@types/node": "latest",
		"@types/vscode": "1.76.0",
		"typescript": "latest",
		"vsce": "latest"
	}
}
